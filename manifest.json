{
    "manifest_version" : 2,
    "name" : "freshtab",
    "version" : "1.0.0",
    "description" : "A chrome extension that displays random photos from Unsplash in new tabs",
    "icons" : {
        "16": "icons/16.png",
        "32": "icons/16.png",
        "48": "icons/16.png",
        "64": "icons/16.png",
        "128": "icons/16.png"

    },
    "chrome_url_overrides" : {
        "newtab" : "index.html"
    },
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "permissions" : ["storage", "unlimitedStorage"],
    "background" : {
        "scripts" : ["js/background.js"],
        "persistant" : false 
    },
    "minimum_chrome_version" : "60"

}